<h1>Post New Dish</h1>

<% @dish.errors.full_messages.each do |msg| %>
  <p><%= msg %></p>
<% end %>

<form action="/dishes" method="post">
  <label for="">Title</label>
  <input type="text" name="title" value="<%= @dish.title %>">
  <label for="">Image Url</label>
  <input type="text" name="image_url" value="<%= @dish.image_url %>">
  <button>create</button>
</form>